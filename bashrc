# Who doesn't love aliases
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias back='cd -'
alias ls='ls -G'
alias ll='ls -Gll'
alias la='ls -Gla'
alias lla='ls -Glla'
alias ps='ps -ax'
alias grep='grep --color=auto'
alias finder='open .'
alias reload='source ~/.bash_profile'

source ~/.uniqrc
