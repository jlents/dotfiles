#!/bin/bash

# This script just echos 1000 lines of blank and clears the screen
# It's good for separating output, like with log files and things
# I want this to run the command that was called before this one
PREV=$_
COUNTER=0
while [  $COUNTER -lt 1000 ]; do
    echo ""
    let COUNTER=COUNTER+1
done
clear
echo "${PREV}"
exit
